<div class="container">
  <div class="quiz-container">
    <ul id="quiz-list">
    </ul>
  </div>
  <div class="question-container">
    <div class="stats-container">
      <ul class="quiz-stats">
        <li class="current-quiz">Current Quiz</li>
        <li class="stat-num">Correct: <span class="correct">0</span></li>
        <li class="stat-num">Incorrect: <span class="incorrect">0</span></li>
        <li class="correctness"></li>
      </ul>
    </div>
    <div class="question-holder"></div>
  </div>
</div>


<script id="quiz-template" type="text/template">
    <li>
      <div class="quiz-template">
        <a class="quiz-link" href="#"></a>
      </div>
    </li>
</script>

<script id="question-template" type="text/template">
  <div>
    <h1 class="title"></h1>
    <form method="post">
      <input type="hidden" name="question_id" class="question-id">
      <input type="hidden" name="session_key" class="session_key">
      <input type="submit" value="Submit">
    </form>
  </div>
</script>

<script id="choice-template" type="text/template">
  <div>
    <input type="radio" class="choice" value=0 ><span class="choice-label"></span>
  </div>
